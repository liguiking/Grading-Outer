<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.easytnt.grading.domain.paper.Test" table="test">
		<id name="testId" type="java.lang.Long">
			<column name="test_id" />
			<generator class="identity" />
		</id>
		<property name="testOid" type="java.lang.Long">
			<column name="test_oid" />
		</property>
		<one-to-one name="subject" class="com.easytnt.grading.domain.paper.Subject"
				fetch="select" cascade="none" constrained="true"></one-to-one>
				
		<property name="testName" type="java.lang.String">
			<column name="test_name" />
		</property>
		
		<property name="testFrome" type="java.util.Date">
			<column name="test_frome" />
		</property>
		
		<property name="testTo" type="java.util.Date">
			<column name="test_to" />
		</property>
		
		<property name="testYear" type="java.lang.Integer">
			<column name="test_year" />
		</property>
		
		<property name="testMonth" type="java.lang.Integer">
			<column name="test_month" />
		</property>
		
		<property name="testWeek" type="java.lang.Integer">
			<column name="test_week" />
		</property>
		
		<set name="paperInfos" table="test_used_paper" inverse="true" cascade="none">
            <key column="test_id"></key>
            <many-to-many class="com.easytnt.grading.domain.paper.PaperInfo" column="paper_id"></many-to-many>
        </set>
		
	</class>
</hibernate-mapping>