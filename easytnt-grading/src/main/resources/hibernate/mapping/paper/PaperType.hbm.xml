<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.easytnt.grading.domain.paper.PaperType" table="properties_dict">
		<id name="prodictId" type="java.lang.Long">
			<column name="prodict_id" />
			<generator class="identity" />
		</id>
		<property name="prodictCatagory" type="java.lang.String">
			<column name="prodict_catagory" />
		</property>
		<property name="typeValue" type="java.lang.String">
			<column name="prodict_code" />
		</property>
		<property name="typeName" type="java.lang.String">
			<column name="prodict_name" />
		</property>
	</class>
</hibernate-mapping>